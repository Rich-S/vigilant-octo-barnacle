<div class="project_panel">

  <div class="panel panel-default" id="project_box_a">
    <div class="panel-heading" id="project_box_b">
      <%= link_to 'New Project', new_project_path, :remote => true %>
    </div>
  </div>

  <% @projects.each do |project| %>
  <div class="panel panel-default" id="project_box_a" data-id="<%= project.id %>">
    <div class="panel-heading" id="project_box_b">
      <%= project.name %>
    </div>  
    <div class="panel-body">
      <%= project.description %>
    </div>
  </div>
  <% end %>

</div>

